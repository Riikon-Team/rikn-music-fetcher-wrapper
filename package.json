{
  "name": "rikn-music-fetcher",
  "version": "1.0.0",
  "description": "A TypeScript wrapper for Discord bots, unifying music fetching from YouTube, SoundCloud, Spotify with ytmusic-api, yt-dlp, lrclib. Robust fallback & link parsing for reliable music search, download, streaming.",
  "main": "index.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:watch": "node --experimental-vm-modules node_modules/.bin/jest --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/.bin/jest --coverage"
  },
  "keywords": [
    "music-fetcher",
    "youtube-api",
    "soundcloud-downloader",
    "spotify-parser",
    "music-downloader",
    "audio-stream",
    "lyrics-api",
    "yt-dlp-wrapper",
    "multi-source-music",
    "robust-api",
    "typescript-library",
    "npm-music",
    "song-search",
    "track-fetch",
    "fallback-music"
  ],
  "author": "Riikon Team",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.2",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.13",
    "axios": "^1.12.2",
    "node-fetch": "^3.3.2"
  }
}
